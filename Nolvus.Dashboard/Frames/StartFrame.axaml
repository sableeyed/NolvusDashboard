<UserControl
    x:Class="Nolvus.Dashboard.Frames.StartFrame"
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Background="Transparent">

    <Border Background="Transparent">
        <Image
            Width="520"
            Height="180"
            Stretch="Uniform"
            HorizontalAlignment="Center"
            VerticalAlignment="Center">
            <Image.Source>
                <SvgImage Source="avares://NolvusDashboard/Assets/background-nolvus.svg" />
            </Image.Source>
        </Image>
    </Border>
</UserControl>
